import viscojapan as vj

site = '_KMN'
#site = 'ULAB'
#site = 'J765'
#site = 'J550'
site = 'J347'

#for cmpt in 'e', 'n', 'u':
for cmpt in ['e']:
    plt = vj.inv.PredictedTimeSeriesPlotter('../pred_disp/~pred_disp.db')

    plt.plot_cumu_disp(site, cmpt)
    plt.plt.savefig('{site}.{cmpt}.cumu.png'.format(site=site, cmpt=cmpt))
    plt.plt.show()
    plt.plt.close()

    plt.plot_post_disp(site, cmpt)
    plt.plt.savefig('{site}.{cmpt}.png'.format(site=site, cmpt=cmpt))
    plt.plt.show()
    plt.plt.close()
